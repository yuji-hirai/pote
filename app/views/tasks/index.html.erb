<h2>スケジュール一覧</h2>

<table> 
    <thead>
        <tr>
            <th>id</th>
            <th>タイトル</th>
            <th>開始日</th>
            <th>終了日</th>
            <th>終日</th>
            <th>スケジュール更新日時</th>
            <th>作成日</th>
        </tr>
    </thead>
    <tbody>
        <% @tasks.each do |task| %>
            <tr>
                <td><%= task.id %></td>
                <td><%= task.title %></td>
                <td><%= task.start_time %></td>
                <td><%= task.end_time %></td>
                <td><%= task.all_day %></td>
                <td><%= task.updated_at %></td>
                <td><%= task.created_at %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<p><%= link_to "スケジュール新規登録", :new_task %></p>